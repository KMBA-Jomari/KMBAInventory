<div id="<%= dom_id supplier %>">
  <p>
    <strong>Supplier name:</strong>
    <%= supplier.supplier_name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= supplier.address %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= supplier.phone_number %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= supplier.email %>
  </p>

  <p>
    <strong>Other details:</strong>
    <%= supplier.other_details %>
  </p>

</div>
